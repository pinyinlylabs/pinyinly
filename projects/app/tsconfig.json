{
  "extends": "../../tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "preserve",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "../../.moon/cache/types/projects/app",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "target": "ESNext",
    "types": [
      "@pinyinly/eslint-rules/nameof.d.ts"
    ]
  },
  "include": [
    "*",
    ".expo/types/**/*.ts",
    "src/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "eslint.config.mjs"
  ],
  "references": [
    {
      "path": "./api"
    },
    {
      "path": "./bin"
    },
    {
      "path": "./src"
    },
    {
      "path": "./test"
    },
    {
      "path": "../audio-sprites"
    },
    {
      "path": "../eslint-rules"
    },
    {
      "path": "../lib"
    },
    {
      "path": "../mdx"
    }
  ]
}
