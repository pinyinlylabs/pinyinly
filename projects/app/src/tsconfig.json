{
  "extends": "../../../tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "preserve",
    "lib": ["DOM", "ES2022"],
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "outDir": "../../../.moon/cache/types/projects/app-src",
    "paths": {
      "@/*": ["./*"]
    },
    "resolveJsonModule": true,
    "target": "ESNext",
    "types": ["@pinyinly/eslint-rules/nameof.d.ts", "@pinyinly/mdx/types"]
  },
  "include": [
    "../.expo/types/**/*.ts",
    "../expo-env.d.ts",
    "../nativewind-env.d.ts",
    "*",
    "**/*",
    "**/*.json"
  ],
  "exclude": [],
  "references": [
    {
      "path": "../../lib"
    }
  ]
}
