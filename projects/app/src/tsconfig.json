{
  "extends": "../../../tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "preserve",
    "lib": ["DOM", "ES2022"],
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "outDir": "../../../.moon/cache/types/projects/app-src",
    "paths": {
      "@/*": ["*"],
      "@pinyinly/lib/*": ["../../lib/src/*"]
    },
    "resolveJsonModule": true,
    "target": "ESNext"
  },
  "include": [
    "../.expo/types/**/*.ts",
    "../expo-env.d.ts",
    "../nativewind-env.d.ts",
    "*",
    "**/*",
    "**/*.json"
  ],
  "references": [
    {
      "path": "../../lib"
    }
  ]
}
