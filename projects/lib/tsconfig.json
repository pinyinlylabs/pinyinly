{
  "extends": "../../tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["ESNext"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "../../.moon/cache/types/projects/lib",
    "target": "ESNext",
    "types": ["node"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.mts",
    "**/*.js",
    "**/*.cjs",
    "**/*.mjs"
  ],
  "exclude": ["dist", "build", "node_modules"],
  "references": [
    {
      "path": "./src"
    }
  ]
}
